<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:SCIMS.Views"
            x:Class="SCIMS.Pages.IssuesPage"
            Title="Issues">
    <ContentPage Title="Open">
        <local:IssuesListView State="Open" />
    </ContentPage>
    
    <ContentPage Title="Closed">
        <local:IssuesListView State="Closed" />
    </ContentPage>
    
    <ContentPage Title="All">
        <local:IssuesListView State="All" />
    </ContentPage>
</TabbedPage>